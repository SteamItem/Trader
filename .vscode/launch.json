{
  // Use IntelliSense to learn about possible attributes.
  // Hover to view descriptions of existing attributes.
  // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [{
    "type": "node",
    "request": "launch",
    "name": "Api",
    "program": "./src/server.ts",
    "runtimeExecutable": "nodemon",
    "restart": true,
    "cwd": "${workspaceFolder}/api",
    "env": {
      "NODE_ENV": "development",
      "DB_URL": "mongodb://localhost:27017/trader",
      "RDB_URL": "postgres://postgres:secret@localhost:5432/Trader",
      "PUPPET_API": "http://localhost:3003",
      "BOT_API": "http://localhost:3002",
      "TELEGRAM_TOKEN": "TELEGRAM_TOKEN",
      "TELEGRAM_CHAT_ID": "TELEGRAM_CHAT_ID",
      "PORT": "3001"
    }
  }, {
    "type": "node",
    "request": "launch",
    "name": "Puppet",
    "program": "./src/server.ts",
    "runtimeExecutable": "nodemon",
    "restart": true,
    "cwd": "${workspaceFolder}/puppet",
    "env": {
      "NODE_ENV": "development",
      "SAVE_SCREENSHOT": "true",
      "HEADLESS": "false",
      "PORT": "3003"
    }
  }, {
    "type": "node",
    "request": "launch",
    "name": "Bot",
    "program": "./src/server.ts",
    "runtimeExecutable": "nodemon",
    "restart": true,
    "cwd": "${workspaceFolder}/bot",
    "env": {
      "NODE_ENV": "development",
      "DB_URL": "mongodb://localhost:27017/trader",
      "RDB_URL": "postgres://postgres:secret@localhost:5432/Trader",
      "TELEGRAM_TOKEN": "TELEGRAM_TOKEN",
      "TELEGRAM_CHAT_ID": "TELEGRAM_CHAT_ID",
      "PORT": "3002"
    }
  }, {
    "type": "node",
    "request": "launch",
    "name": "Gateway",
    "program": "./server.js",
    "runtimeExecutable": "nodemon",
    "restart": true,
    "cwd": "${workspaceFolder}/gateway",
    "env": {
      "NODE_ENV": "development",
      "HOST": "localhost",
      "WEB_API": "http://localhost:3001",
      "BOT_API": "http://localhost:3002",
      "PUPPET_API": "http://localhost:3003",
      "PORT": "3000"
    }
  }, {
    "name": "Admin",
    "type": "node",
    "request": "launch",
    "cwd": "${workspaceRoot}/admin",
    "runtimeExecutable": "npm",
    "env": {
      "VUE_APP_API_URL": "http://localhost:3000"
    },
    "runtimeArgs": [
      "run-script", "serve"
    ],
    "port": 5858
  }],
  "compounds": [{
    "name": "Debug All",
    "configurations": ["Api", "Puppet", "Bot", "Gateway", "Admin"]
  }]
}