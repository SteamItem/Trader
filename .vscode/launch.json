{
  // Use IntelliSense to learn about possible attributes.
  // Hover to view descriptions of existing attributes.
  // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [{
    "type": "node",
    "request": "launch",
    "name": "Server",
    "program": "./src/server.ts",
    "runtimeExecutable": "nodemon",
    "restart": true,
    "console": "integratedTerminal",
    "internalConsoleOptions": "neverOpen",
    "cwd": "${workspaceFolder}/TraderServer",
    "env": {
      "NODE_ENV": "development",
      "CORS_WHITELIST": "http://localhost:8080",
      "DB_URL": "mongodb://localhost:27017/trader",
      "RDB_URL": "postgres://postgres:secret@localhost:5432/Trader",
      "PUPPET_API": "http://localhost:3001",
      "BOT_API": "http://localhost:3002",
      "TELEGRAM_TOKEN": "TELEGRAM_TOKEN",
      "TELEGRAM_CHAT_ID": "TELEGRAM_CHAT_ID",
      "PORT": "3000"
    }
  }, {
    "type": "node",
    "request": "launch",
    "name": "Puppet",
    "program": "./src/server.ts",
    "runtimeExecutable": "nodemon",
    "restart": true,
    "console": "integratedTerminal",
    "internalConsoleOptions": "neverOpen",
    "cwd": "${workspaceFolder}/TraderPuppet",
    "env": {
      "NODE_ENV": "development",
      "SAVE_SCREENSHOT": "true",
      "HEADLESS": "false",
      "PORT": "3001"
    }
  }, {
    "type": "node",
    "request": "launch",
    "name": "Bot",
    "program": "./src/server.ts",
    "runtimeExecutable": "nodemon",
    "restart": true,
    "console": "integratedTerminal",
    "internalConsoleOptions": "neverOpen",
    "cwd": "${workspaceFolder}/TraderBot",
    "env": {
      "NODE_ENV": "development",
      "DB_URL": "mongodb://localhost:27017/trader",
      "RDB_URL": "postgres://postgres:secret@localhost:5432/Trader",
      "TELEGRAM_TOKEN": "TELEGRAM_TOKEN",
      "TELEGRAM_CHAT_ID": "TELEGRAM_CHAT_ID",
      "PORT": "3002"
    }
  }, {
    "name": "Web",
    "type": "node",
    "request": "launch",
    "cwd": "${workspaceRoot}/TraderWeb",
    "runtimeExecutable": "npm",
    "env": {
      "VUE_APP_API_URL": "http://localhost:3000"
    },
    "runtimeArgs": [
      "run-script", "serve"
    ],
    "port": 5858
  }],
  "compounds": [{
    "name": "Debug All",
    "configurations": ["Server", "Puppet", "Bot", "Web"]
  }]
}